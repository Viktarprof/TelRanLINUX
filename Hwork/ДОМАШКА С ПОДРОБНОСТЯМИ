   0 mkdir -p /home/user/start.   ПРОПИСАЛ ПУТЬ И СОЗДАЛ ПАПКИ. КОНЕЧНАЯ ПАПКА СТАРТ
   1 touch /home/user/start/1.txt СОЗДАЛ ФАЙЛ 1.ТХТ ПРОПИСАВ К НЕЙ ПУТЬ 
   2 echo "Start" > /home/user/start/1.txt. СОЗДАЛ СЛОВО СТАРТ И ПРОПИСАЛ ЗАПИСАТЬ СЛОВО В ФАЙЛЕ. СЛОВА В КАВЫЧКАХ 
   3 date. ВЫВОД НА ЭКРАН ДАТЫ
   4 date >> /home/user/start/1.txt. ДОПИСАТЬ ДАТУ В ФАЙЛ
   5 mkdir -p /opt/task СОЗДАЛ ПАПКУ ТАСК ПРОПИСАВ ПУТЬ И НЕ ЗАБЫВАЕМ  НАПИСАТЬ -Р
   6 cp /home/user/start/1.txt /opt/task/start.txt СКОПИРОВАЛ ФАЙЛ И ВСТАВИЛ ФАЙЛ С ПЕРЕИМЕНОВАНИЕМ 
   7 cd / ОТКРЫЛ КОРЕНЬ
   8 ls -la |tail -4 >> /opt/task/start.txt ПОКАЗАЛ ЧТО ВНУТРИ КОРНЯ, ПОКАЗАЛ СКРЫТЫЕ ФАЙЛЫ И ПОДРОБНУЮ ИНФ-Ю. В ПОДРОБНОЙ ИНФОРМАЦИИ ВЫБРАЛ ПОСЛЕДНИЕ ЧЕТЫРЕ СТРОКИ ДОПИСАЛ ИХ В ФАЙЛ 
   9 cat /opt/task/start.txt ОТКРЫЛ ФАЙЛ КОТОРЫЙ ДОПИСАЛ
   10 history ПОКАЗАЛ СТОРИЮ
   11 history > /tmp/history. ДОПИСАЛ ФАЙЛ В ПАПКУ 
